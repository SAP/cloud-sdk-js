{
  "name": "get-changelog",
  "version": "2.12.0",
  "description": "Gets the current changelog",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "compile": "tsc -p tsconfig.json",
    "lint": "eslint --ignore-pattern '!index.ts' && prettier --check '**/*.ts'",
    "lint:fix": "eslint --ignore-pattern '!index.ts' --fix --quiet && prettier --write '**/*.ts'",
    "check:dependencies": "depcheck .",
    "package": "ncc build lib/build-packages/get-changelog/index.js --out ../../.github/actions/get-changelog/",
    "all": "yarn && yarn run compile && yarn run lint && yarn run package"
  },
  "dependencies": {
    "@actions/core": "^1.9.1",
    "@changesets/get-release-plan": "^3.0.16",
    "semver": "^7.3.8"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.36.0",
    "depcheck": "^1.4.3",
    "eslint": "^8.30.0",
    "prettier": "^2.8.1",
    "typescript": "^4.9.4"
  }
}
