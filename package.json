{
  "name": "sap-cloud-sdk",
  "private": true,
  "sideEffects": false,
  "description": "SAP Cloud SDK for JavaScript",
  "homepage": "https://community.sap.com/topics/cloud-sdk",
  "license": "Apache-2.0",
  "keywords": [
    "sap-cloud-sdk",
    "cloud-sdk",
    "sap-cloud-platform"
  ],
  "repository": "github:SAP/cloud-sdk",
  "scripts": {
    "test": "npm run test:unit && npm run test:integration && npm run test:type",
    "test:unit": "npx lerna run test --ignore \"@sap-cloud-sdk/integration-tests\" --ignore \"@sap-cloud-sdk/type-tests\"",
    "test:integration": "npx lerna run test --scope \"@sap-cloud-sdk/integration-tests\"",
    "test:type": "npx lerna run test --scope \"@sap-cloud-sdk/type-tests\"",
    "generate:test-services": "npx ts-node scripts/generate-test-services.ts && npm run lint:fix && lerna link --force-local",
    "compile": "npx lerna run compile",
    "doc": "node scripts/generate-docs.js",
    "version": "node scripts/after-bump.js && npm run doc && git add .",
    "lint": "npx eslint --ext .ts .",
    "lint:fix": "npm run lint -- --fix",
    "lint:fix-cached": "npm run lint:fix -- --cache",
    "postinstall": "npx lerna bootstrap --force-local --strict",
    "check:license": "npx license-checker -direct --excludePackages \"@sap/edm-converters@1.0.21;@sap/xsenv@2.2.0;weak-map@1.0.5\" --onlyAllow \"MIT;Apache;ISC;BSD;WTFPL;Artistic;CC-BY;CC0;Unlicense;Public Domain\"",
    "check:vulnerability": "npx audit-ci -h",
    "bump:patch": "npx lerna version patch --force-publish -y",
    "bump:minor": "npx lerna version minor --force-publish -y"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^8.3.8",
    "@types/uuid": "^7.0.2",
    "@types/voca": "^1.4.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "audit-ci": "^2.5.1",
    "compare-versions": "^3.6.0",
    "eslint": "^6.8.0",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-unused-imports": "^0.1.2",
    "glob": "^7.1.6",
    "lerna": "^3.20.2",
    "license-checker": "^25.0.1",
    "nock": "^12.0.3",
    "prettier": "^2.0.2",
    "ts-node": "^8.8.1",
    "typedoc": "^0.16.11",
    "typedoc-plugin-lerna-packages": "^0.3.0",
    "typescript": "~3.8.3"
  },
  "dependencies": {}
}
