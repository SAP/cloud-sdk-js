{
  "name": "sap-cloud-sdk",
  "version": "1.47.1",
  "private": true,
  "sideEffects": false,
  "description": "SAP Cloud SDK for JavaScript",
  "homepage": "https://sap.github.io/cloud-sdk/docs/js/getting-started",
  "license": "Apache-2.0",
  "keywords": [
    "sap-cloud-sdk",
    "cloud-sdk",
    "sap-cloud-platform"
  ],
  "repository": "github:SAP/cloud-sdk-js",
  "workspaces": [
    "packages/analytics",
    "packages/core",
    "packages/eslint-config",
    "packages/generator",
    "packages/test-util",
    "packages/util",
    "packages/openapi-generator",
    "packages/generator-common",
    "test-packages/integration-tests",
    "test-packages/e2e-tests",
    "test-packages/test-services",
    "test-packages/test-services-e2e",
    "test-packages/type-tests"
  ],
  "scripts": {
    "postinstall": "yarn compile && yarn run readme",
    "test": "yarn workspaces run test",
    "test:unit": "yarn lerna run test --ignore \"@sap-cloud-sdk/integration-tests\" --ignore \"@sap-cloud-sdk/type-tests\" --ignore \"@sap-cloud-sdk/e2e-tests\" --ignore \"@sap-cloud-sdk/nightly-tests\"",
    "test:integration": "yarn integration-tests test",
    "test:type": "yarn type-tests test",
    "test:e2e": "yarn e2e-tests test",
    "generate:test-services:v2": "yarn generate v2 && yarn lint:fix && yarn --frozen-lockfile",
    "generate:test-services:v4": "yarn generate v4 && yarn lint:fix && yarn --frozen-lockfile",
    "generate:test-services:openapi": "yarn generate openapi && yarn lint:fix && yarn --frozen-lockfile",
    "generate:test-services:e2e": "yarn generate e2e && yarn lint:fix && yarn --frozen-lockfile",
    "generate:test-services": "yarn generate all && yarn lint:fix && yarn --frozen-lockfile",
    "generate": "yarn workspace @sap-cloud-sdk/generator run compile && yarn ts-node scripts/generate-test-services.ts",
    "compile": "yarn lerna run compile",
    "doc": "ts-node scripts/generate-docs.ts",
    "version": "ts-node scripts/after-bump.ts && yarn generate:test-services && yarn doc && git add .",
    "readme": "ts-node scripts/replace-common-readme.ts && yarn lerna run readme",
    "lint": "yarn eslint --ext .ts . && yarn prettier . -c",
    "lint:fix": "yarn eslint --ext .ts . --fix && yarn prettier . -w",
    "lint:fix-cached": "yarn eslint --ext .ts . --fix --cache && yarn prettier . -w",
    "check:circular": "yarn madge --extensions ts --circular ./packages --exclude 'test-services|environment-accessor|\\.d\\.ts'",
    "check:dependencies": "yarn workspaces run check:dependencies",
    "check:license": "ts-node scripts/check-licenses.ts",
    "check:test-service": "yarn generate:test-services && yarn ts-node scripts/check-test-service-for-changes.ts",
    "analytics": "yarn workspace @sap-cloud-sdk/analytics",
    "core": "yarn workspace @sap-cloud-sdk/core",
    "generator": "yarn workspace @sap-cloud-sdk/generator",
    "generator-common": "yarn workspace @sap-cloud-sdk/generator-common",
    "openapi-generator": "yarn workspace @sap-cloud-sdk/openapi-generator",
    "test-util": "yarn workspace @sap-cloud-sdk/test-util",
    "util": "yarn workspace @sap-cloud-sdk/util",
    "e2e-tests": "yarn workspace @sap-cloud-sdk/e2e-tests",
    "integration-tests": "yarn workspace @sap-cloud-sdk/integration-tests",
    "type-tests": "yarn workspace @sap-cloud-sdk/type-tests"
  },
  "devDependencies": {
    "@types/jest": "^27.0.0",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/license-checker": "^25.0.1",
    "@types/mock-fs": "^4.13.0",
    "@types/uuid": "^8.0.0",
    "@types/voca": "^1.4.0",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.1-alpha.1",
    "compare-versions": "^3.6.0",
    "depcheck": "^1.0.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-tsdoc": "^0.2.14",
    "eslint-plugin-unused-imports": "^1.0.0",
    "glob": "^7.1.6",
    "jest": "^26.4.2",
    "jest-junit": "^12.0.0",
    "lerna": "^4.0.0",
    "license-checker": "^25.0.1",
    "madge": "^5.0.0",
    "prettier": "^2.0.2",
    "ts-jest": "^26.4.1",
    "ts-node": "^10.0.0",
    "typedoc": "^0.21.0",
    "typescript": "~4.3.4"
  },
  "dependencies": {},
  "resolutions": {
    "@types/node": "^12.0.0",
    "agent-base": ">=6.0.2",
    "color-string": ">=1.5.5",
    "dot-prop": ">=5.1.1",
    "netmask": ">=2.0.1 ",
    "path-parse": ">=1.0.7",
    "postcss": ">=8.2.10",
    "tar": ">=6.1.2",
    "trim-newlines": ">=4.0.1"
  }
}
