{
  "name": "sap-cloud-sdk",
  "private": true,
  "sideEffects": false,
  "description": "SAP Cloud SDK for JavaScript",
  "homepage": "https://www.sap.com/cloud-sdk",
  "license": "Apache-2.0",
  "keywords": [
    "sap-cloud-sdk",
    "cloud-sdk",
    "sap-cloud-platform"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "test": "npm run test:unit && npm run test:integration && npm run test:type",
    "test:unit": "npx lerna run test --ignore \"@sap-cloud-sdk/integration-tests\" --ignore \"@sap-cloud-sdk/type-tests\"",
    "test:integration": "npx lerna run test --scope \"@sap-cloud-sdk/integration-tests\"",
    "test:type": "npx lerna run test --scope \"@sap-cloud-sdk/type-tests\"",
    "postinstall": "npx lerna bootstrap --hoist --force-local --strict",
    "generate:test-services": "npx ts-node generate-test-services.ts && npm run lint:fix && lerna link --force-local",
    "compile": "npx lerna run compile",
    "lint": "npm run tslint && npm run prettier",
    "lint:fix": "npm run tslint:fix && npm run prettier:fix",
    "prettier": "prettier -l \"**/*.ts\"",
    "prettier:fix": "prettier --write \"**/*.ts\"",
    "tslint": "npx lerna run tslint",
    "tslint:fix": "npx lerna run tslint:fix",
    "doc": "node generate-docs.js",
    "check:license": "npx license-checker -direct --excludePackages \"@sap/edm-converters@1.0.19;@sap/xsenv@2.2.0;weak-map@1.0.5\" --onlyAllow \"MIT;Apache;ISC;BSD;WTFPL;Artistic;CC-BY;CC0;Unlicense\"",
    "check:vulnerability": "npx audit-ci -m"
  },
  "dependencies": {
    "@sap/edm-converters": "1.0.19",
    "@sap/xsenv": "2.2.0",
    "axios": "0.19.0",
    "bignumber.js": "8.1.1",
    "chalk": "^2.4.2",
    "jsonwebtoken": "8.5.1",
    "moment": "2.24.0",
    "opossum": "4.0.0",
    "rambda": "2.14.5",
    "ts-morph": "4.0.1",
    "voca": "1.4.0",
    "winston": "^3.2.1",
    "yargs": "14.0.0"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^8.3.6",
    "@types/lolex": "^3.1.1",
    "@types/nock": "^9.3.1",
    "@types/rewire": "^2.5.28",
    "@types/voca": "^1.4.0",
    "@types/yargs": "^13.0.2",
    "audit-ci": "^2.5.1",
    "dtslint": "^2.0.2",
    "eclint": "^2.8.1",
    "glob": "^7.1.4",
    "lerna": "^3.16.4",
    "license-checker": "^25.0.1",
    "lolex": "^4.2.0",
    "nock": "^10.0.6",
    "pkginfo": "^0.4.1",
    "prettier": "^1.18.2",
    "rewire": "^4.0.1",
    "ts-node": "^8.6.2",
    "tslint": "5.20.1",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-plugin-blank-line": "0.0.9",
    "typedoc": "^0.15.0",
    "typedoc-plugin-lerna-packages": "^0.2.2",
    "typescript": "3.7.4"
  }
}
