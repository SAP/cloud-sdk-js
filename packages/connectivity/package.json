    "axios": "^0.26.0",
    "http-proxy-agent": "^5.0.0",
    "https-proxy-agent": "^5.0.0",
    "jsonwebtoken": "^8.5.1",
    "opossum": "^6.0.0"
  },
  "devDependencies": {
    "@types/opossum": "^6.2.0",
    "base64url": "^3.0.1",
