{
  "name": "@sap-cloud-sdk/core",
  "version": "1.47.1",
  "description": "SAP Cloud SDK for JavaScript core",
  "homepage": "https://sap.github.io/cloud-sdk/docs/js/overview-cloud-sdk-for-javascript",
  "license": "Apache-2.0",
  "keywords": [
    "sap-cloud-sdk",
    "cloud-sdk",
    "sap-cloud-platform",
    "core"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.js.map",
    "dist/**/*.d.ts",
    "dist/**/*.d.ts.map",
    "usage-analytics.js"
  ],
  "repository": "github:SAP/cloud-sdk-js",
  "scripts": {
    "compile": "yarn tsc -b",
    "prepare": "yarn compile",
    "postinstall": "node usage-analytics.js",
    "test": "yarn jest",
    "coverage": "yarn jest --coverage",
    "check:dependencies": "depcheck ."
  },
  "dependencies": {
    "@sap-cloud-sdk/analytics": "^1.47.1",
    "@sap-cloud-sdk/util": "^1.47.1",
    "@sap/xsenv": "^3.0.0",
    "@types/jsonwebtoken": "^8.3.8",
    "axios": "^0.21.1",
    "bignumber.js": "^9.0.0",
    "http-proxy-agent": "^4.0.1",
    "https-proxy-agent": "^5.0.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.0",
    "opossum": "^6.0.0",
    "uuid": "^8.2.0",
    "voca": "^1.4.0"
  },
  "devDependencies": {
    "@sap-cloud-sdk/test-util": "^1.47.1",
    "@types/opossum": "^4.1.1",
    "nock": "^13.0.11"
  }
}
