{
  "name": "@sap-cloud-sdk/core",
  "version": "1.25.0",
  "description": "SAP Cloud SDK for JavaScript core",
  "homepage": "https://community.sap.com/topics/cloud-sdk",
  "license": "Apache-2.0",
  "keywords": [
    "sap-cloud-sdk",
    "cloud-sdk",
    "sap-cloud-platform",
    "core"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "v4.d.ts",
    "v4.js",
    "dist/**/*.js",
    "dist/**/*.js.map",
    "dist/**/*.d.ts",
    "dist/**/*.d.ts.map",
    "usage-analytics.js"
  ],
  "repository": "github:SAP/cloud-sdk",
  "scripts": {
    "compile": "yarn tsc -b",
    "prepare": "yarn compile",
    "postinstall": "node usage-analytics.js",
    "test": "yarn jest --coverage",
    "test:local": "yarn jest --config ./jest-local.json",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand --config ./jest-local.json",
    "test:watch-debug": "node --inspect-brk node_modules/.bin/jest --watch --runInBand --config ./jest-local.json",
    "test:debug-name-regex": "node -r ts-node/register --inspect-brk ./node_modules/.bin/jest --config jest-local.json --runInBand --testNamePattern=\"$PATTERN\""
  },
  "dependencies": {
    "@sap-cloud-sdk/analytics": "^1.25.0",
    "@sap-cloud-sdk/util": "^1.25.0",
    "@sap/xsenv": "^3.0.0",
    "axios": "^0.19.2",
    "bignumber.js": "^9.0.0",
    "http-proxy-agent": "^4.0.1",
    "https-proxy-agent": "^5.0.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "~2.27.0",
    "opossum": "^5.0.0",
    "rambda": "^5.0.0",
    "url": "^0.11.0",
    "uuid": "^8.2.0",
    "voca": "^1.4.0"
  },
  "devDependencies": {
    "@sinonjs/fake-timers": "^6.0.1",
    "@types/jest": "^25.1.4",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/sinonjs__fake-timers": "^6.0.0",
    "jest": "^26.1.0",
    "jest-junit": "^11.1.0",
    "ts-jest": "^25.4.0",
    "typescript": "~3.9.6"
  }
}
