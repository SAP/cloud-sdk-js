{
  "name": "@sap-cloud-sdk/generator-common",
  "version": "2.2.0",
  "description": "SAP Cloud SDK for JavaScript common functions of OData client generator and OpenAPI clint generator.",
  "homepage": "https://sap.github.io/cloud-sdk/docs/js/overview-cloud-sdk-for-javascript",
  "license": "Apache-2.0",
  "keywords": [
    "sap-cloud-sdk",
    "cloud-sdk",
    "sap-cloud-platform",
    "generator-common"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.js.map",
    "dist/**/*.d.ts",
    "dist/**/*.d.ts.map",
    "internal.js",
    "internal.d.ts"
  ],
  "repository": "github:SAP/cloud-sdk-js",
  "scripts": {
    "compile": "yarn tsc -b",
    "prepare": "yarn compile",
    "test": "yarn jest",
    "coverage": "yarn jest --coverage",
    "check:dependencies": "depcheck ."
  },
  "dependencies": {
    "@sap-cloud-sdk/util": "^2.2.0",
    "axios": "^0.26.0",
    "fast-levenshtein": "~3.0.0",
    "fs-extra": "^10.0.0",
    "glob": "^7.1.6",
    "typescript": "~4.6.2"
  },
  "devDependencies": {
    "jest-extended": "^2.0.0",
    "mock-fs": "^5.0.0",
    "nock": "^13.0.11"
  }
}
