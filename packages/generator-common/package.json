{
  "name": "@sap-cloud-sdk/generator-common",
  "version": "1.42.0",
  "description": "SAP Cloud SDK for JavaScript common functions of OData client generator and OpenAPI clint generator.",
  "homepage": "https://sap.github.io/cloud-sdk/docs/js/overview-cloud-sdk-for-javascript",
  "license": "Apache-2.0",
  "keywords": [
    "sap-cloud-sdk",
    "cloud-sdk",
    "sap-cloud-platform",
    "generator-common"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.js.map",
    "dist/**/*.d.ts",
    "dist/**/*.d.ts.map"
  ],
  "repository": "github:SAP/cloud-sdk-js",
  "scripts": {
    "compile": "yarn tsc -b",
    "prepare": "yarn compile",
    "test": "yarn jest --coverage --runInBand",
    "test:local": "yarn jest",
    "test:debug": "node --inspect-brk ../../node_modules/.bin/jest --runInBand",
    "check:dependencies": "depcheck ."
  },
  "dependencies": {
    "@sap-cloud-sdk/util": "^1.42.0",
    "fs-extra": "^10.0.0",
    "typescript": "~4.1.2"
  },
  "devDependencies": {
    "nock": "^13.0.11",
    "mock-fs": "^4.13.0",
    "jest-extended": "^0.11.5"
  }
}
