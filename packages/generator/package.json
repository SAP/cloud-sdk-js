{
  "name": "@sap-cloud-sdk/generator",
  "version": "2.2.0",
  "description": "SAP Cloud SDK for JavaScript OData client generator",
  "homepage": "https://sap.github.io/cloud-sdk/docs/js/overview-cloud-sdk-for-javascript",
  "license": "Apache-2.0",
  "keywords": [
    "sap-cloud-sdk",
    "cloud-sdk",
    "sap-cloud-platform",
    "generator"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "generate-odata-client": "./dist/generator-cli.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.js.map",
    "dist/**/*.d.ts",
    "dist/**/*.d.ts.map",
    "internal.d.ts",
    "internal.js"
  ],
  "repository": "github:SAP/cloud-sdk-js",
  "scripts": {
    "compile": "yarn tsc -b",
    "prepare": "yarn compile",
    "test": "yarn jest",
    "coverage": "yarn jest --coverage",
    "check:dependencies": "depcheck . --ignores='@sap-cloud-sdk/odata-v2,@sap-cloud-sdk/odata-v4'"
  },
  "dependencies": {
    "@sap-cloud-sdk/generator-common": "^2.2.0",
    "@sap-cloud-sdk/odata-common": "^2.2.0",
    "@sap-cloud-sdk/odata-v2": "^2.2.0",
    "@sap-cloud-sdk/odata-v4": "^2.2.0",
    "@sap-cloud-sdk/util": "^2.2.0",
    "@sap/edm-converters": "~1.0.21",
    "@types/fs-extra": "^9.0.1",
    "@types/glob": "^7.1.2",
    "fast-xml-parser": "^4.0.1",
    "fs-extra": "^10.0.0",
    "glob": "^7.1.6",
    "ts-morph": "^14.0.0",
    "typescript": "~4.6.2",
    "voca": "^1.4.0",
    "yargs": "^17.0.1"
  },
  "devDependencies": {
    "@types/yargs": "^17.0.0",
    "execa": "^5.0.0",
    "jest-extended": "^2.0.0",
    "mock-fs": "^5.0.0",
    "nock": "^13.0.11"
  }
}
