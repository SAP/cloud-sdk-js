{
  "name": "@sap-cloud-sdk/openapi-generator",
  "version": "1.41.0",
  "description": "SAP Cloud SDK for JavaScript OpenApi client generator",
  "homepage": "https://sap.github.io/cloud-sdk/docs/js/overview-cloud-sdk-for-javascript",
  "license": "Apache-2.0",
  "keywords": [
    "sap-cloud-sdk",
    "cloud-sdk",
    "sap-cloud-platform",
    "generator"
  ],
  "repository": "github:SAP/cloud-sdk-js",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "generate-openapi-client": "./bin/run"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "bin/openapi-jar"
  ],
  "oclif": {
    "bin": "generate-openapi-client",
    "commands": "./dist/cli"
  },
  "scripts": {
    "compile": "yarn tsc -b",
    "prepare": "yarn compile",
    "readme": "oclif-dev readme",
    "test": "yarn jest --coverage ",
    "test:local": "yarn jest",
    "test:debug": "node --inspect-brk ../../node_modules/.bin/jest --runInBand",
    "test:watch-debug": "node --inspect-brk ../../node_modules/.bin/jest --watch --runInBand",
    "check:dependencies": "depcheck . --ignore-bin-package=true --ignores=@oclif/plugin-*"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.0.2",
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.14.0",
    "@sap-cloud-sdk/util": "^1.41.0",
    "@types/js-yaml": "^4.0.0",
    "execa": "^5.0.0",
    "glob": "^7.1.6",
    "js-yaml": "^4.0.0",
    "openapi-types": "^8.0.0",
    "swagger2openapi": "^7.0.4"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "mock-fs": "^4.13.0",
    "typescript": "~4.1.2"
  }
}
