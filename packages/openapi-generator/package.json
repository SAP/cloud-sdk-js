{
  "name": "@sap-cloud-sdk/openapi-generator",
  "version": "1.39.0",
  "description": "SAP Cloud SDK for JavaScript OpenApi client generator",
  "homepage": "https://sap.github.io/cloud-sdk/docs/js/overview-cloud-sdk-for-javascript",
  "license": "Apache-2.0",
  "keywords": [
    "sap-cloud-sdk",
    "cloud-sdk",
    "sap-cloud-platform",
    "generator"
  ],
  "repository": "github:SAP/cloud-sdk-js",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "generate-openapi-client": "./bin/run"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "bin/openapi-jar"
  ],
  "oclif": {
    "bin": "generate-openapi-client",
    "warn-if-update-available": {
      "timeoutInDays": 7
    },
    "macos": {
      "identifier": "com.sap.generate-openapi-client"
    },
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-not-found",
      "@oclif/plugin-warn-if-update-available"
    ]
  },
  "scripts": {
    "compile": "yarn tsc -b && cp -r -f src/templates dist",
    "prepare": "yarn compile",
    "readme": "oclif-dev readme",
    "test": "yarn jest --coverage ",
    "test:local": "yarn jest",
    "test:debug": "node --inspect-brk ../../node_modules/.bin/jest --runInBand",
    "test:watch-debug": "node --inspect-brk ../../node_modules/.bin/jest --watch --runInBand",
    "check:dependencies": "depcheck . --ignore-bin-package=true --ignores=@oclif/plugin-*"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.0.2",
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.14.0",
    "@oclif/parser": "^3.8.4",
    "@oclif/plugin-autocomplete": "^0.3.0",
    "@oclif/plugin-help": "^3.2.2",
    "@oclif/plugin-not-found": "^1.2.3",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "@openapitools/openapi-generator-cli": "^2.1.4",
    "@sap-cloud-sdk/util": "^1.39.0",
    "@types/js-yaml": "^4.0.0",
    "cli-ux": "^5.4.5",
    "execa": "^5.0.0",
    "glob": "^7.1.6",
    "js-yaml": "^4.0.0",
    "openapi-types": "^7.0.1",
    "swagger2openapi": "^7.0.4",
    "voca": "^1.4.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "mock-fs": "^4.13.0",
    "typescript": "~4.1.2"
  }
}
