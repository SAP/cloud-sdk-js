{
  "name": "@sap-cloud-sdk/rest-generator",
  "version": "1.32.1",
  "description": "SAP Cloud SDK for JavaScript REST api generator",
  "homepage": "https://www.sap.com/cloud-sdk",
  "license": "Apache-2.0",
  "keywords": [
    "sap-cloud-sdk",
    "cloud-sdk",
    "sap-cloud-platform",
    "generator"
  ],
  "private": true,
  "repository": "github:SAP/cloud-sdk",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "generate-rest-client": "./dist/generator-cli.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "compile": "yarn tsc -b",
    "prepare": "yarn compile",
    "test": "yarn jest --coverage ",
    "test:local": "yarn jest",
    "test:debug": "node --inspect-brk ../../node_modules/.bin/jest --runInBand",
    "test:watch-debug": "node --inspect-brk ../../node_modules/.bin/jest --watch --runInBand",
    "generate:test-services": "ts-node src/generator-cli.ts -i test/resources/test-apis -o test/test-services",
    "check:dependencies": "depcheck . --ignore-bin-package=true"
  },
  "dependencies": {
    "@openapitools/openapi-generator-cli": "^2.1.4",
    "@sap-cloud-sdk/core": "^1.32.1",
    "@sap-cloud-sdk/util": "^1.32.1",
    "execa": "^4.0.3",
    "fs-extra": "^9.0.0",
    "ts-morph": "^8.1.0",
    "typescript": "~4.0.2",
    "yargs": "^16.0.3"
  }
}
