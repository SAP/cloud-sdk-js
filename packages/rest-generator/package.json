{
  "name": "@sap-cloud-sdk/rest-generator",
  "version": "1.32.1",
  "description": "SAP Cloud SDK for JavaScript REST api generator",
  "homepage": "https://www.sap.com/cloud-sdk",
  "license": "Apache-2.0",
  "keywords": [
    "sap-cloud-sdk",
    "cloud-sdk",
    "sap-cloud-platform",
    "generator"
  ],
  "private": true,
  "repository": "github:SAP/cloud-sdk",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "sap-cloud-sdk-rest": "./bin/run"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "compile": "yarn tsc -b",
    "prepare": "yarn compile",
    "test": "yarn jest --coverage ",
    "test:local": "yarn jest",
    "test:debug": "node --inspect-brk ../../node_modules/.bin/jest --runInBand",
    "test:watch-debug": "node --inspect-brk ../../node_modules/.bin/jest --watch --runInBand",
    "generate:test-services": "ts-node src/commands/generate-rest-client.ts -i test/resources/test-apis -o test/test-services",
    "check:dependencies": "depcheck . --ignore-bin-package=true --ignores=@oclif/plugin-*"
  },
  "oclif": {
    "commands": "./dist/commands",
    "bin": "sap-cloud-sdk-rest",
    "warn-if-update-available": {
      "timeoutInDays": 7
    },
    "macos": {
      "identifier": "com.sap.sap-cloud-sdk-rest"
    },
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-not-found",
      "@oclif/plugin-warn-if-update-available"
    ]
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.0.2",
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.14.0",
    "@oclif/parser": "^3.8.4",
    "@oclif/plugin-autocomplete": "^0.2.0",
    "@oclif/plugin-help": "^3.1.0",
    "@oclif/plugin-not-found": "^1.2.3",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "@openapitools/openapi-generator-cli": "^2.1.4",
    "@sap-cloud-sdk/util": "^1.32.1",
    "cli-ux": "^5.4.5",
    "execa": "^4.1.0",
    "openapi-types": "^7.0.1",
    "typescript": "~4.1.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2"
  }
}
