{
  "name": "@sap-cloud-sdk/temporal-de-serializers",
  "version": "2.1.0",
  "description": "SAP Cloud SDK for JavaScript temporal-based middleware for date/time (de-)serialization",
  "homepage": "https://sap.github.io/cloud-sdk/docs/js/overview-cloud-sdk-for-javascript",
  "license": "Apache-2.0",
  "keywords": [
    "sap-cloud-sdk",
    "cloud-sdk",
    "sap-cloud-platform",
    "temporal-de-serializers"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.js.map",
    "dist/**/*.d.ts",
    "dist/**/*.d.ts.map",
    "internal.js",
    "internal.d.ts"
  ],
  "repository": "github:SAP/cloud-sdk-js",
  "scripts": {
    "compile": "yarn tsc -b",
    "prepare": "yarn compile",
    "test": "yarn jest",
    "coverage": "yarn jest --coverage",
    "lint": "eslint --ext .ts . && prettier . --config='../../.prettierrc' --ignore-path='../../.prettierignore' -c",
    "lint:fix": "eslint --ext .ts . --fix && prettier . --config='../../.prettierrc' --ignore-path='../../.prettierignore' -w",
    "check:dependencies": "depcheck .",
    "check:public-api": "ts-node ../../scripts/check-public-api-cli.ts",
    "readme": "ts-node ../../scripts/replace-common-readme.ts"
  },
  "dependencies": {
    "@js-temporal/polyfill": "^0.3.0",
    "@sap-cloud-sdk/odata-common": "^2.1.0"
  },
  "devDependencies": {
    "typescript": "~4.6.2"
  }
}
