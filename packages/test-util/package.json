{
  "name": "@sap-cloud-sdk/test-util",
  "version": "1.18.0",
  "description": "SAP Cloud SDK for JavaScript test utilities",
  "homepage": "https://www.sap.com/cloud-sdk",
  "license": "Apache-2.0",
  "keywords": [
    "sap-cloud-sdk",
    "cloud-sdk",
    "sap-cloud-platform",
    "test-util"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.js.map",
    "dist/**/*.d.ts",
    "dist/**/*.d.ts.map"
  ],
  "repository": "github:SAP/cloud-sdk",
  "scripts": {
    "compile": "npx tsc",
    "prepare": "npm run compile",
    "test": "npx jest --coverage --runInBand",
    "test:local": "npx jest --config ./jest-local.json",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand --config ./jest-local.json",
    "tslint": "npx tslint \"**/*.ts\" -e \"**/node_modules/**\" -e \"**/*.d.ts\"",
    "tslint:fix": "npm run tslint -- --fix"
  },
  "dependencies": {
    "@sap-cloud-sdk/core": "^1.18.0",
    "@sap-cloud-sdk/util": "^1.18.0"
  },
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "jest": "^25.1.0",
    "jest-junit": "^10.0.0",
    "ts-jest": "^25.2.1",
    "tslint": "6.0.0",
    "typescript": "3.7.4"
  }
}
