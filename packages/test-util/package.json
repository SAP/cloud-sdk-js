{
  "name": "@sap-cloud-sdk/test-util",
  "version": "1.32.1",
  "description": "SAP Cloud SDK for JavaScript test utilities",
  "homepage": "https://community.sap.com/topics/cloud-sdk",
  "license": "Apache-2.0",
  "keywords": [
    "sap-cloud-sdk",
    "cloud-sdk",
    "sap-cloud-platform",
    "test-util"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.js.map",
    "dist/**/*.d.ts",
    "dist/**/*.d.ts.map"
  ],
  "repository": "github:SAP/cloud-sdk",
  "scripts": {
    "compile": "yarn tsc -b",
    "prepare": "yarn compile",
    "test": "yarn jest --coverage --runInBand",
    "test:local": "yarn jest",
    "test:debug": "node --inspect-brk ../../node_modules/.bin/jest --runInBand",
    "check:dependencies": "depcheck ."
  },
  "dependencies": {
    "@sap-cloud-sdk/core": "^1.32.1",
    "@sap-cloud-sdk/util": "^1.32.1"
  }
}
