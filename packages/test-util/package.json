{
  "name": "@sap-cloud-sdk/test-util",
  "version": "1.50.0",
  "description": "SAP Cloud SDK for JavaScript test utilities",
  "homepage": "https://sap.github.io/cloud-sdk/docs/js/overview-cloud-sdk-for-javascript",
  "license": "Apache-2.0",
  "keywords": [
    "sap-cloud-sdk",
    "cloud-sdk",
    "sap-cloud-platform",
    "test-util"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.mjs",
    "dist/**/*.js.map",
    "dist/**/*.d.ts",
    "dist/**/*.d.ts.map"
  ],
  "repository": "github:SAP/cloud-sdk-js",
  "scripts": {
    "compile": "yarn tsc -b",
    "prepare": "yarn compile",
    "wrap-esm": "yarn gen-esm-wrapper ./dist/index ./dist/index.mjs",
    "test": "yarn jest",
    "coverage": "yarn jest --coverage",
    "check:dependencies": "depcheck ."
  },
  "dependencies": {
    "@sap-cloud-sdk/connectivity": "^1.50.0",
    "@sap-cloud-sdk/util": "^1.50.0"
  },
  "devDependencies": {
    "mock-fs": "^5.0.0"
  }
}
