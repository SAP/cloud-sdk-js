    "chalk": "^4.1.0",
    "logform": "^2.2.0",
    "promise.allsettled": "^1.0.4",
    "voca": "^1.4.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "nock": "^13.0.11",
    "typescript": "~4.6.2"
  }
