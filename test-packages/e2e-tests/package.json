{
  "name": "@sap-cloud-sdk/e2e-tests",
  "version": "2.5.0",
  "description": "End to end tests of the SAP Cloud SDK for JavaScript",
  "homepage": "https://sap.github.io/cloud-sdk/docs/js/overview-cloud-sdk-for-javascript",
  "license": "Apache-2.0",
  "private": true,
  "repository": "github:SAP/cloud-sdk-js",
  "scripts": {
    "pretest:e2e": "yarn deploy && yarn start:service-server && yarn start:proxy-server && yarn start:mail-server",
    "posttest:e2e": "yarn stop:service-server && yarn stop:service-server && yarn stop:mail-server",
    "start:service-server": "pm2 start server.js || pm2 restart server.js",
    "stop:service-server": "pm2 stop server.js",
    "start:proxy-server": "pm2 start test/on-prem/proxy-server.js || pm2 restart test/on-prem/proxy-server.js",
    "stop:proxy-server": "pm2 stop test/on-prem/proxy-server.js",
    "start:mail-server": "pm2 start test/mail/mail-server.js || pm2 restart test/mail/mail-server.js",
    "stop:mail-server": "pm2 stop test/mail/mail-server.js",
    "deploy": "cds deploy --to sqlite",
    "test": "yarn test:e2e",
    "test:e2e": "jest",
    "lint": "eslint --ext .ts . && prettier . --config ../../.prettierrc --ignore-path ../../.prettierignore -c",
    "lint:fix": "set TIMING=1 && eslint --ext .ts . --fix --quiet && prettier . --config ../../.prettierrc --ignore-path ../../.prettierignore -w --loglevel error",
    "check:dependencies": "depcheck . --ignores='express,sqlite3,@sap/cds'"
  },
  "devDependencies": {
    "@apidevtools/swagger-parser": "^10.0.2",
    "@sap-cloud-sdk/generator": "^2.5.0",
    "@sap-cloud-sdk/generator-common": "^2.5.0",
    "@sap-cloud-sdk/odata-common": "^2.5.0",
    "@sap-cloud-sdk/odata-v4": "^2.5.0",
    "@sap-cloud-sdk/connectivity": "^2.5.0",
    "@sap-cloud-sdk/http-client": "^2.5.0",
    "@sap-cloud-sdk/openapi-generator": "^2.5.0",
    "@sap-cloud-sdk/test-services-openapi": "^2.5.0",
    "@sap-cloud-sdk/test-services-e2e": "^2.5.0",
    "@sap-cloud-sdk/util": "^2.5.0",
    "@sap/cds": "^6.0.2",
    "@sap/cds-dk": "6.0.2",
    "@types/nodemailer": "^6.4.4",
    "axios": "^0.27.1",
    "execa": "^5.0.0",
    "express": "^4.17.1",
    "fs-extra": "^10.0.0",
    "http-proxy-middleware": "^2.0.6",
    "json-schema-faker": "^0.5.0-rcv.44",
    "moment": "^2.29.4",
    "nodemailer": "6.7.7",
    "openapi-backend": "^5.0.0",
    "pm2": "^5.1.2",
    "smtp-server": "3.11.0",
    "sqlite3": "^5.0.2"
  },
  "cds": {
    "cdsc": {
      "odataFormat": "structured"
    },
    "requires": {
      "db": {
        "kind": "sqlite",
        "model": "*"
      }
    }
  }
}
