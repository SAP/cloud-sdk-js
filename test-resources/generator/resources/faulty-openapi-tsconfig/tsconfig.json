{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["non-exisiting-lib"],
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "diagnostics": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "inlineSources": false,
    "strict": true
  },
  "include": ["**/*.ts"],
  "exclude": ["dist/**/*", "node_modules/**/*"]
}
