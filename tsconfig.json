{
  "compilerOptions": {
    "target": "es5" ,
    "module": "commonjs",
    "lib": [
      "esnext"
    ],
    "sourceMap": true  ,
    "declaration": true  ,
    "declarationMap": true ,
        "incremental": true,
    "skipLibCheck": true,
       "strictNullChecks": true  ,

    "moduleResolution": "node"  ,

    "allowSyntheticDefaultImports": true ,
    "esModuleInterop": true,
    "inlineSources": false
  }
}
