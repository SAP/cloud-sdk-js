{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "globalDependencies": [
    ".prettierrc",
    ".eslintrc.js",
    ".prettierignore",
    ".gitignore",
    "tsconfig.json"
  ],
  "pipeline": {
    "compile": {
      "dependsOn": ["^compile", "check:dependencies"]
    },
    "test": {
      "dependsOn": ["^compile"]
    },
    "readme": {
      "dependsOn": ["@sap-cloud-sdk/util#compile"],
      "outputs": ["README.md"]
    },
    "check:dependencies": {
      "dependsOn": ["^compile"],
      "outputs": []
    },
    "check:public-api": {
      "dependsOn": ["@sap-cloud-sdk/util#compile"],
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "lint:fix": {
      "outputs": []
    },
    "generate:v2": {
      "dependsOn": [
        "@sap-cloud-sdk/generator#compile",
        "@sap-cloud-sdk/util#compile"
      ],
      "outputs": ["v2/**"]
    },
    "generate:v4": {
      "dependsOn": [
        "@sap-cloud-sdk/generator#compile",
        "@sap-cloud-sdk/util#compile"
      ],
      "outputs": ["v4/**"]
    },
    "generate:e2e": {
      "dependsOn": [
        "@sap-cloud-sdk/generator#compile",
        "@sap-cloud-sdk/util#compile"
      ],
      "outputs": ["TripPin/**", "v4/**"]
    },
    "generate:openapi": {
      "dependsOn": [
        "@sap-cloud-sdk/openapi-generator#compile",
        "@sap-cloud-sdk/util#compile"
      ],
      "outputs": ["openapi/**"]
    },
    "generate:common": {
      "dependsOn": ["@sap-cloud-sdk/generator#compile", "compile"],
      "outputs": ["test/**"]
    }
  }
}
