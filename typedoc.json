{
  "mode": "modules",
  "out": "docs/api/1.21.2",
  "exclude": [
    "**/index.ts",
    "scripts/*.ts",
    "*.ts",
    "**/*.d.ts",
    "**/*.spec.ts",
    "**/node_modules/**/*",
    "**/dist/**/*",
    "**/test/**/*",
    "test-packages/**/*",
    "packages/core/src/odata/v4/**/*",
    "packages/core/src/odata/common/**/*"
  ],
  "name": "SAP Cloud SDK for JavaScript / TypeScript",
  "readme": "DOCUMENTATION.md",
  "theme": "default",
  "includeVersion": true,
  "ignoreCompilerErrors": true,
  "hideGenerator": true,
  "excludePrivate": true,
  "excludeNotExported": true,
  "target": "ES5",
  "moduleResolution": "node",
  "module": "commonjs",
  "listInvalidSymbolLinks": true,
  "lernaExclude": [
    "@sap-cloud-sdk/integration-tests",
    "@sap-cloud-sdk/test-services",
    "@sap-cloud-sdk/type-tests"
  ]
}
